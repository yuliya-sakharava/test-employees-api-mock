    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "array",
    "default": [],
    "title": "Root Schema",
    "items": {
        "type": "object",
        "default": {},
        "title": "A Schema",
        "required": [
            "employeeId",
            "name",
            "organization",
            "role"
        ],
        "properties": {
            "employeeId": {
                "type": "integer",
                "default": 0,
                "title": "The employeeId Schema",
                "examples": [
                    1
